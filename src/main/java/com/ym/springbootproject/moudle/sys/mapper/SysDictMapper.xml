<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ym.springbootproject.moudle.sys.mapper.SysDictMapper">

	<select id="getSysDictByType" resultType="map">

		select code,name,type,parent_code from
			sys_dict where delflag = '0' and type = #{type}
	</select>

</mapper> 